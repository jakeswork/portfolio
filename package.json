{
  "name": "portfolio",
  "version": "0.1.0",
  "private": true,
  "homepage": "http://jakeswork.github.io/portfolio",
  "dependencies": {
    "@types/react-syntax-highlighter": "^11.0.4",
    "circular-std": "^1.0.0",
    "classnames": "^2.2.6",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "normalize-jss": "^4.0.0",
    "react": "^16.10.2",
    "react-dom": "^16.10.2",
    "react-icons": "^3.7.0",
    "react-jss": "^10.0.0",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.2.0",
    "react-syntax-highlighter": "^12.2.1",
    "react-tooltip": "^4.2.7",
    "react-typist": "^2.0.5",
    "typescript": "3.6.3"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "predeploy": "yarn run build",
    "deploy": "gh-pages -d build",
    "lint": "eslint . --ext .ts,.tsx"
  },
  "eslintConfig": {
    "extends": "react-app",
    "overrides": [
      {
        "files": [
          "**/*.ts?(x)"
        ]
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/classnames": "^2.2.9",
    "@types/enzyme": "^3.10.3",
    "@types/enzyme-adapter-react-16": "^1.0.5",
    "@types/jest": "24.0.18",
    "@types/node": "12.7.11",
    "@types/react": "16.9.5",
    "@types/react-dom": "16.9.1",
    "@types/react-icons": "^3.0.0",
    "@types/react-jss": "^8.6.4",
    "@types/react-router-dom": "^5.1.0",
    "@types/react-typist": "^2.0.1",
    "eslint": "^6.5.1",
    "gh-pages": "^2.1.1",
    "husky": ">=1",
    "lint-staged": ">=8",
    "prettier": "1.18.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,css,json,md}": [
      "prettier --write",
      "git add"
    ]
  }
